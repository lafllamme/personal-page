{
  "version": 2,
  "builds": [
    {
      "src": "cms/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/cms/api/$1"
    },
    {
      "src": "/admin(.*)",
      "dest": "/cms/admin$1"
    },
    {
      "src": "/(.*)",
      "dest": "/cms/$1"
    }
  ]
} 